{
    "C_Cpp.default.cppStandard": "c++14",
    "C_Cpp.default.intelliSenseMode": "gcc-x64",
    "editor.lineNumbers": "relative",
    "editor.autoClosingBrackets": "never",
    "editor.autoClosingQuotes": "never",
    "editor.renderWhitespace": "none",
    "editor.folding": false,
    "editor.fontFamily": "'Ubuntu Mono', 'Liberation Mono', 'monospace', monospace, 'Droid Sans Fallback'",
    "editor.rulers": [
        {
            "column": 80,
            "color": "#2e2e2c"
        },
    ],
    "editor.minimap.enabled": false,
    "editor.hideCursorInOverviewRuler": true,
    "jupyter.sendSelectionToInteractiveWindow": true,
    "python.pythonPath": "C:\\v\\env1\\Scripts\\python.exe",
    "python.venvPath": "~/v",
    "python.terminal.launchArgs": [
        "-c",
        "\"from IPython import start_ipython; start_ipython()\""
    ],
    "terminal.integrated.shell.windows": "C:\\WINDOWS\\System32\\cmd.exe",
    "terminal.integrated.shellArgs.linux": ["-l"],
    "window.menuBarVisibility": "toggle",
    "vim.insertModeKeyBindingsNonRecursive": [
        {
        "before": ["j", "k"],
        "after": ["<Esc>"]
        },
        {
        "before": ["k", "j"],
        "after": ["<Esc>"]
        }
    ],
    "workbench.activityBar.visible": false,
    "workbench.colorTheme": "Gruvbox Material Light",
    "workbench.preferredLightColorTheme": "Gruvbox Material Light",
    "workbench.colorCustomizations": {
        "panel.border": "#3b3b39",
        "sideBar.border": "#3b3b39"
    },
    "window.zoomLevel": 1,
    "jupyter.askForKernelRestart": false,
    "cmake.configureOnOpen": true,
    "cmake.buildBeforeRun": false,
    "editor.tabSize": 8,
    "editor.cursorStyle": "line",
    "editor.insertSpaces": false,
    "editor.wordSeparators": "/\\()\"':,.;<>~!@#$%^&*|+=[]{}`?-",
    "editor.wordWrap": "off"
}